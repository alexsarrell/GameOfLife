                        <Button text="{-}" styleClass="windowButton" onAction="#onIconified"/>
                        <Button text="{X}" styleClass="windowButton"
                                id="closeButton" onAction="#onCloseButton"/>
                </ToolBar>
        </top>
        <center>
                <BorderPane>
                        <top>
                                <ToolBar styleClass="mainToolbar">
                                        
                                </ToolBar>
                        </top>
                </BorderPane>
        </center>
</BorderPane>